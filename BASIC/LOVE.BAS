10 DEFINT A-Z:DEFDBL P,T:W=30:H=90:Y0=70:PI=3.141592654#
20 DIM M(256),G(290),D(576):P=0
30 DATA 0,3,12,15,48,51,60,63,192,195,204,207,240,243,252,255
40 FOR I=0 TO 15:READ G(I):NEXT I
50 FOR I=0 TO 15:FOR J=0 TO 7:N=G(I) OR G(J)*256:OF=I*16+J
60 M(OF)=N:M(OF+8)=N OR &HC000:NEXT J:NEXT I
70 VSEG=VARSEG(D(0)):IF VSEG<>0 THEN DEF SEG=VSEG
80 BLOAD "LOVE.BIN",VARPTR(D(0)):DEF SEG
90 KEY OFF:SCREEN 1:COLOR ,1:CLS:PI=3.141592654#
100 CIRCLE (160-W,Y0),W,2,-PI*2,-PI,.9:PAINT (160-W,Y0-3),2
110 CIRCLE (160+W,Y0),W,2,-PI*2,-PI,.9:PAINT (160+W,Y0-3),2
120 FOR Y=1 TO H:X=INT(W*2*COS(Y*PI/2/H)+.5)
130 LINE (-X+160,Y+Y0)-(X+160,Y+Y0),2
140 NEXT Y:G(0)=48*2:G(1)=48
150 V=0:VSEG=VARSEG(V):IF VSEG<>0 THEN DEF SEG=VSEG
160 PV=VARPTR(V):PV1=PV+1:X=30:Y=50:GOSUB 200:Y=102:GOSUB 200
170 X=241:Y=50:GOSUB 200:Y=102:GOSUB 200
180 DEF SEG:WHILE INPUT$(1)="":WEND:SCREEN 0:WIDTH 80:CLS:END
200 FOR I=0 TO 143:V=D(P):P=P+1
210 G(2+I*2)=M(PEEK(PV)) AND &H5555:G(2+I*2+1)=M(PEEK(PV1)) AND &H5555
220 NEXT I:PUT (X,Y),G:RETURN

