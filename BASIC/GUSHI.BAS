10 DEFINT A-Z:DIM M(256),G(74),G0(34),G1(34),IM(4000):PRINT "Loading...";
20 VSEG=VARSEG(IM(0)):IF VSEG=0 THEN DEF SEG ELSE DEF SEG=VSEG
30 BLOAD "GUSHI.BIN",VARPTR(IM(0))
40 DATA 0,3,12,15,48,51,60,63,192,195,204,207,240,243,252,255
50 FOR I=0 TO 15:READ G(I):NEXT I
60 FOR I=0 TO 15:FOR J=0 TO 7:N=G(I) OR G(J)*256:OF=I*16+J
70 M(OF)=N:M(OF+8)=N OR &HC000:NEXT J:NEXT I
80 SCREEN 2:KEY OFF:CLS
90 P=VARPTR(IM(0))+(((IM(0)+7)\8)*IM(1)\2+2)*2
100 Y=30:X=50:G(0)=48:G(1)=24:FOR I=0 TO 3
120 FOR J=2 TO 73:G(J)=M(PEEK(P)):P=P+1:NEXT J
130 PUT (X,Y),G:X=X+56:NEXT I
140 Y=58:X=122:G(0)=32:G(1)=12:FOR I=0 TO 1
150 FOR J=2 TO 25:G(J)=M(PEEK(P)):P=P+1:NEXT J
160 PUT (X,Y),G:X=X+48:NEXT I
170 G0(0)=32:G0(1)=16:G1(0)=32:G1(1)=16:G(0)=32:G(1)=16
180 FOR I=2 TO 33:G0(I)=M(PEEK(P)):P=P+1:NEXT I
190 FOR I=2 TO 33:G1(I)=M(PEEK(P)):P=P+1:NEXT I
200 FOR R=0 TO 3:Y=85+R*23:X=42:FOR I=0 TO 6
210 FOR J=2 TO 33:G(J)=M(PEEK(P)):P=P+1:NEXT J
220 PUT (X,Y),G:X=X+32:NEXT I:IF R AND 1 THEN PUT (X,Y),G1 ELSE PUT (X,Y),G0
230 NEXT R:PUT (320,30),IM:DEF SEG:WHILE INPUT$(1)="":WEND
240 CLS:SCREEN 0:WIDTH 80:END
