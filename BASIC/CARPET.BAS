10 DEFINT A-Z:DIM G(1643):KEY OFF:SCREEN 1:CLS
20 S=2*3^4:X=(320-S)/2:Y=(199-S)/2
30 LINE(X,Y)-(X+5,Y+5),3,BF:LINE(X+2,Y+2)-(X+3,Y+3),0,BF
40 FOR N=0 TO 2:GOSUB 100:NEXT N
50 WHILE INPUT$(1)="":WEND:SCREEN 0:WIDTH 80:CLS:END
100 S=3^N*6:GET (X,Y)-(X+S-1,Y+S-1),G
110 PUT (X+S,Y),G:PUT (X+S*2,Y),G:PUT (X,Y+S),G:PUT (X+S*2,Y+S),G
120 FOR I=0 TO 2:PUT (X+S*I,Y+S*2),G:NEXT I:RETURN
